package exercise2;

class PrintCheckerPattern {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		System.out.println("* \t\t * \t\t * \t\t * \t\t * \t\t");
		System.out.println("\t * \t\t * \t\t * \t\t * \t\t *");
		System.out.println("* \t\t * \t\t * \t\t * \t\t * \t\t");
		System.out.println("\t * \t\t * \t\t * \t\t * \t\t *");
		System.out.println("* \t\t * \t\t * \t\t * \t\t * \t\t");
		

	}

}

